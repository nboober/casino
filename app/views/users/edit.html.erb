<button><%= link_to "Home", home_path %></button>
<button><%= link_to "Back To Profile", user_path(@user)%></button>


<h2>Edit Profile</h2>

<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br><br>


  <%= f.label :age %>
  <%= f.text_field :age %><br/><br/>

  <%= f.submit "Complete Profile Edit" %>

<% end %>